{
  "name": "@sobol/daemon",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "daemon:build-dev": "tsc --watch --project tsconfig.build.json",
    "daemon:run": "yarn run nodemon ./dist/index.js --watch dist/",
    "daemon:debug": "yarn run nodemon --inspect=9090 ./dist/index.js --watch dist/",
    "daemon:debug-break": "yarn run nodemon --inspect-brk=localhost:9222 ./dist/index.js",
    "daemon:deploy-commands": "yarn node ./dist/scripts/deploy-commands.js",
    "daemon:generate-data": "yarn node ./dist/scripts/generate-data.js",
    "daemon:clear-data": "rm database.sqlite"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.9.6",
    "@types/uuid": "^8.3.1",
    "nodemon": "^2.0.12",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@sobol/daemon-types": "workspace:*",
    "@veramo/core": "^3.0.0",
    "@veramo/credential-w3c": "^3.0.0",
    "@veramo/data-store": "^3.0.0",
    "@veramo/did-manager": "^3.0.0",
    "@veramo/did-provider-ethr": "^3.0.0",
    "@veramo/did-provider-web": "^3.0.0",
    "@veramo/did-resolver": "^3.0.0",
    "@veramo/key-manager": "^3.0.0",
    "@veramo/kms-local": "^3.0.0",
    "cors": "^2.8.5",
    "did-resolver": "3.1.2",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "ethr-did-resolver": "^4.3.4",
    "express": "^4.17.1",
    "http": "^0.0.1-security",
    "lodash": "^4.17.21",
    "sqlite3": "^5.0.2",
    "typeorm": "0.2.37",
    "uuid": "^8.3.2",
    "web-did-resolver": "^2.0.4"
  }
}
