{
  "name": "@sobol/daemon",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "daemon:build-dev": "tsc --watch --project tsconfig.build.json",
    "daemon:run": "yarn run nodemon --inspect ./dist/index.js --watch dist/*",
    "daemon:debug": "yarn run nodemon --inspect ./dist/index.js",
    "daemon:debug-break": "yarn run nodemon --inspect-brk=localhost:9222 ./dist/index.js",
    "daemon:deploy-commands": "yarn node ./dist/deploy-commands.js"
  },
  "devDependencies": {
    "@types/node": "^16.9.6",
    "nodemon": "^2.0.12",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@veramo/core": "^3.0.0",
    "@veramo/credential-w3c": "^3.0.0",
    "@veramo/data-store": "^3.0.0",
    "@veramo/did-manager": "^3.0.0",
    "@veramo/did-provider-ethr": "^3.0.0",
    "@veramo/did-provider-web": "^3.0.0",
    "@veramo/did-resolver": "^3.0.0",
    "@veramo/key-manager": "^3.0.0",
    "@veramo/kms-local": "^3.0.0",
    "did-resolver": "3.1.1",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "ethr-did-resolver": "^4.3.4",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.37",
    "web-did-resolver": "^2.0.4"
  }
}
